<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xmlns:context="http://www.springframework.org/schema/context"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd
 http://www.springframework.org/schema/context
 http://www.springframework.org/schema/context/spring-context.xsd">
 
 <bean class="useingConstructor.Laptop" id="laptop" autowire="useingConstructor">
<property name="name" value="Lenovo">
</property>
 <property name="price" value="42000">
 </property>
 </bean>
 <bean class="useingConstructor.Processor" id="processor">
<property name="processorName" value="core i3">
</property>
 <property name="RAM" value="4 GB">
 </property>
 </bean>
 
 
 </beans>
 
 
 